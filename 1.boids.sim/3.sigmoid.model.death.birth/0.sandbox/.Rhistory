base.death <- .05
linear.impact.growth <- -0.001
linear.impact.death <- 0.0005
sd.overdisp <- .5
exp(rnorm(1000, mean = 0, sd = sd.overdisp)) %>% hist
# carrying capacity is at intersection
K <- (base.growth - base.death) / (base.death * linear.impact.death - base.growth * linear.impact.growth)
pop[1] <- runif(1, K/10, K) %>% round
for(y in 2:n.years){
pop[y] <-
pop[y-1] +
rpois(1, lambda = (base.growth + base.growth * linear.impact.growth * pop[y-1]) * pop[y-1] * exp(rnorm(1, mean = 0, sd = sd.overdisp))) -
rpois(1, lambda = (base.death + base.death * linear.impact.death * pop[y-1]) * pop[y -1] * exp(rnorm(1, mean = 0, sd = sd.overdisp)))
}
plot(pop, type = 'l', ylim = c(0, max(c(K, pop))))
